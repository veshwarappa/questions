select
  meeting_id,
  submitted_payload
from public.meeting_records
where meeting_id = '<MEETING_ID>';

select
  meeting_id,
  submitted_payload->>'startDateTime' as start_dt,
  submitted_payload->>'endDateTime' as end_dt,
  submitted_payload->>'createdDateTime' as created_dt,
  submitted_payload->>'originalStartTimeZone' as start_tz,
  submitted_payload->>'originalEndTimeZone' as end_tz
from public.meeting_records
where meeting_id = '<MEETING_ID>';


select
  step_name,
  status,
  started_at,
  completed_at,
  step_metadata
from public.pipeline_events
where meeting_record_id = '<MEETING_ID>'
  and step_name = 'microsoft_graph_integration';


select
  meeting_id,
  subject,
  created_at,
  transcript_available_at,
  last_checked_at,
  retry_count
from public.transcripts
where retry_count > 0
order by created_at desc
limit 50;


select
  step_name,
  status,
  started_at,
  completed_at
from public.pipeline_events
where meeting_record_id = '<THAT_MEETING_ID>'
order by started_at;


