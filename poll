ContainerAppConsoleLogs_CL
| where TimeGenerated between (datetime(2026-02-18T17:30:00Z) .. datetime(2026-02-18T19:00:00Z))
| where Log_s has "restrictionNotes" or Log_s has "testbec" or Log_s has "VGhlIGNhbmRpZGF0ZS"
| order by TimeGenerated asc

ContainerAppConsoleLogs_CL
| where Log_s has "difficulty climbing stairs"
| order by TimeGenerated desc

ContainerAppConsoleLogs_CL
| where Log_s has "restrictionNotes"
| order by TimeGenerated desc

| where Log_s has "Restriction"

let t = datetime(2026-02-20T10:15:00Z);   // replace with actual timestamp
ContainerAppConsoleLogs_CL
| where TimeGenerated between (t - 5m .. t + 5m)
| order by TimeGenerated asc
