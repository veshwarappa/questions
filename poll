let t = datetime(2026-02-18T22:52:09Z);
ContainerAppConsoleLogs_CL
| where TimeGenerated between (t - 10m .. t + 10m)
| where Log_s has "AAMkADAyYWI5Nj"   // use the first ~12-15 chars from your meeting_id
| project TimeGenerated, Log_s
| order by TimeGenerated asc



let t = datetime(2026-02-18T22:52:09Z);
ContainerAppConsoleLogs_CL
| where TimeGenerated between (t - 10m .. t + 10m)
| where Log_s has "OneTouch"
   or Log_s has "OT"
   or Log_s has "restriction"
   or Log_s has "restrictionNotes"
   or Log_s has "payload"
   or Log_s has "submit"
   or Log_s has "POST /"   // outbound HTTP logs sometimes look like this
| project TimeGenerated, Log_s
| order by TimeGenerated asc


let t = datetime(2026-02-18T22:52:09Z);
ContainerAppConsoleLogs_CL
| where TimeGenerated between (t - 15m .. t + 15m)
| where Log_s has "POST " or Log_s has "PUT " or Log_s has "PATCH "
| project TimeGenerated, Log_s
| order by TimeGenerated asc


SELECT
  meeting_id,
  payload_submitted_at,
  submitted_payload ? 'restrictionNotes' AS has_restrictionNotes,
  submitted_payload->>'restrictionNotes' AS restrictionNotes_value
FROM public.meeting_records
WHERE meeting_id = '<FULL_MEETING_ID>'
ORDER BY payload_submitted_at DESC
LIMIT 5;


