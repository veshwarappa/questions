select
  meeting_id,
  interview_date,
  start_time,
  end_time,
  transcript_available_at,
  join_url
from transcripts
where join_url like '%meetup-join/19%3ameeting_%'
order by transcript_available_at desc;


select
  meeting_id,
  interview_date,
  start_time,
  end_time,
  transcript_available_at
from transcripts
where join_url like '%meeting_YmQwYTMxNzQtZjQ5YS00OTAxLWI4ODEtMGE2MzgzYWFjMTZk%40thread.v2%'
order by transcript_available_at desc;


select
  meeting_id,
  event_date,
  start_time,
  end_time,
  created_at,
  payload_submitted_at,
  completed_at,
  region,
  environment
from meeting_records
where meeting_id in (
  'MEETING_ID_1',
  'MEETING_ID_2'
)
order by created_at desc;



select
  mr.meeting_id,
  mr.event_date,
  mr.start_time as scheduled_start,
  mr.end_time as scheduled_end,
  mr.created_at as record_created_at,
  t.transcript_available_at,
  t.join_url
from meeting_records mr
join transcripts t on t.meeting_id = mr.meeting_id
where t.join_url like '%meeting_YmQwYTMxNzQtZjQ5YS00OTAxLWI4ODEtMGE2MzgzYWFjMTZk%40thread.v2%'
order by t.transcript_available_at desc;
