SELECT 
    mr.meeting_id,
    mr.payload_submitted_at,
    mr.completed_at,
    t.transcript_available_at,
    t.retry_count,
    t.last_checked_at
FROM public.meeting_records mr
JOIN public.transcripts t 
    ON t.meeting_id = mr.meeting_id
WHERE mr.environment = 'prod'
AND mr.region = 'ANZ'
AND mr.completed_at::date = '2026-02-18'
AND mr.interviewee_email = 'priyanka.khurana@hays.com.au';


let meetingId = "PUT_MEETING_ID_HERE";
ContainerAppConsoleLogs_CL
| where TimeGenerated between (datetime(2026-02-18 00:00:00) .. datetime(2026-02-19 00:00:00))
| where Log_s has meetingId
    or Log_s has "Sending review email"
| project TimeGenerated, Log_s
| order by TimeGenerated asc


