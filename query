ContainerAppConsoleLogs_CL
| where TimeGenerated between (datetime(2026-02-17) .. datetime(2026-02-18))
| where Log_s contains "error" or Log_s contains "exception"
| summarize count() by bin(TimeGenerated, 30m)
| order by TimeGenerated asc


ContainerAppConsoleLogs_CL
| where TimeGenerated between (datetime(2026-02-17) .. datetime(2026-02-18))
| where Log_s contains "transcript"
| order by TimeGenerated asc

ContainerAppConsoleLogs_CL
| where TimeGenerated between (datetime(2026-02-17) .. datetime(2026-02-18))
| where Log_s contains "queue" or Log_s contains "retry"
| summarize count() by bin(TimeGenerated, 30m)
