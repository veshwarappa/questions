SELECT
  mr.region,
  t.meeting_id,
  mr.completed_at AS meeting_finished_at_proxy,
  t.transcript_available_at,
  (t.transcript_available_at - mr.completed_at) AS delay_after_finished,
  t.processed,
  t.retry_count
FROM public.transcripts t
JOIN public.meeting_records mr ON mr.meeting_id = t.meeting_id
WHERE t.transcript_available_at IS NOT NULL
  AND mr.completed_at IS NOT NULL
ORDER BY delay_after_finished DESC
LIMIT 20;
