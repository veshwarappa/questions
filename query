SELECT
  meeting_id,
  created_at,
  last_checked_at,
  transcript_available_at,
  (transcript_available_at - created_at) AS processing_time,
  ROUND(EXTRACT(EPOCH FROM (transcript_available_at - created_at)) / 60.0, 2)
    AS processing_minutes
FROM public.transcripts
WHERE meeting_id = 'AAMkADAy...';

SELECT
  meeting_id,
  event_date,
  start_time,
  end_time
FROM public.meeting_records
WHERE meeting_id = 'AAMkADAy...';
