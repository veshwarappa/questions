SELECT
  mr.meeting_id,
  mr.completed_at,
  t.transcript_available_at,
  ROUND(EXTRACT(EPOCH FROM
    (t.transcript_available_at - mr.completed_at)
  )/60.0,2) AS actual_processing_delay_min
FROM public.transcripts t
JOIN public.meeting_records mr
  ON mr.meeting_id = t.meeting_id
WHERE mr.environment='prod'
  AND mr.region='ANZ'
  AND mr.completed_at::date = '2026-02-17'
ORDER BY actual_processing_delay_min DESC
LIMIT 20;
